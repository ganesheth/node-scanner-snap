{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-console": [
      "off"
    ],
    "global-require":[
      "off"
    ]
  }
}